{
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    },
    {
      "use": "@vercel/static"
    },
    {
      "use": "@vercel/node",
      "entrypoint": "api/counter.js",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/counter",
      "methods": ["GET"],
      "dest": "api/counter.js"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "true",
    "PATH": "/usr/local/bin:/usr/bin:/bin:$PATH"
  },
  "build": {
    "env": {
      "PIP_TARGET": "/vercel/path0/.python_packages",
      "PYTHONPATH": "/vercel/path0/lib/python3.8/site-packages"
    },
    "commands": [
      "apt-get update",
      "apt-get install -y python3",
      "pip install --upgrade pip",
      "pip install -r requirements.txt"
    ]
  }
}
